{
  "expo": {
    "name": "dripApp",
    "slug": "dripApp",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#e63c4b"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "notification": {
      "iosDisplayInForeground": true
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    }
  }
}
